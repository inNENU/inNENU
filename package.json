{
  "name": "innenu",
  "version": "6.0.0",
  "private": true,
  "homepage": "https://innenu.com",
  "bugs": {
    "url": "https://github.com/inNENU/inNENU/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inNENU/inNENU.git"
  },
  "license": "Mr.Hope LICENSE",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mister-hope.com"
  },
  "type": "module",
  "scripts": {
    "build:app": "gulp -f gulpfile.cjs buildApp",
    "build:qq": "gulp -f gulpfile.cjs buildQQ",
    "build:qy": "gulp -f gulpfile.cjs buildNenuyouth",
    "build:wx": "gulp -f gulpfile.cjs buildWechat",
    "changelog": "standard-version --skip.bump --skip.commit --skip.tag",
    "clean": "node scripts/del.js",
    "commit": "pnpm git-cz",
    "dev:app": "gulp -f gulpfile.cjs watchApp",
    "dev:qq": "gulp -f gulpfile.cjs watchQQ",
    "dev:qy": "gulp -f gulpfile.cjs watchNenuyouth",
    "dev:wx": "gulp -f gulpfile.cjs watchWechat",
    "lint": "pnpm lint:prettier --write && pnpm lint:wxs --write && pnpm lint:wxml --write && pnpm lint:eslint --fix && pnpm lint:stylelint --fix",
    "lint:check": "pnpm lint:prettier && pnpm lint:wxs && pnpm lint:wxml && pnpm lint:eslint && pnpm lint:stylelint",
    "lint:eslint": "eslint --ext .ts,.wxs .",
    "lint:prettier": "prettier --check .",
    "lint:stylelint": "stylelint **/*.scss",
    "lint:wxml": "prettier --check --parser html **/*.wxml",
    "lint:wxs": "prettier --check --parser babel-flow **/*.wxs",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "@mptool/all": "0.4.0",
    "miniprogram-recycle-view": "0.1.5",
    "tslib": "2.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@mr-hope/gulp-sass": "3.1.1",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.4.1",
    "commitizen": "4.3.0",
    "cz-git": "1.7.1",
    "del": "7.0.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "gulp": "4.0.2",
    "gulp-rename": "2.0.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "husky": "8.0.3",
    "miniprogram-api-typings": "3.12.0",
    "nano-staged": "0.8.0",
    "postcss": "8.4.28",
    "prettier": "3.0.2",
    "sort-package-json": "2.5.1",
    "standard-version": "9.5.0",
    "stylelint": "15.10.3",
    "stylelint-config-hope": "3.0.0",
    "typescript": "5.2.2"
  },
  "packageManager": "pnpm@8.7.0",
  "nano-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,ts,json,yaml,yml,md}": "prettier --write",
    "*.wxss": "stylelint --fix",
    "*.wxml": "prettier --parser html --write",
    "*.wxs": "prettier --parser babel-flow --write"
  },
  "pnpm": {
    "overrides": {
      "cacache": "^17.0.5",
      "chokidar": "^3.5.3",
      "css": "npm:@adobe/css-tools@^4.3.0",
      "glob-parent": "^6.0.2",
      "glob-stream": "^7.0.0",
      "micromatch": "^4.0.5",
      "yargs-parser": "^21.1.1"
    }
  }
}
