{
  "cSpell.words": [
    "benbu",
    "canonicalize",
    "damnu",
    "danmu",
    "darkmode",
    "elif",
    "errcode",
    "errmsg",
    "gulpfile",
    "inited",
    "innenu",
    "jfif",
    "jingyue",
    "meilisearch",
    "miniapp",
    "miniprogram",
    "mptool",
    "nenu",
    "qqcode",
    "qqid",
    "qrcode",
    "qzone",
    "rmvb",
    "stylelint",
    "wechat",
    "weixin",
    "werun",
    "weui",
    "wxid",
    "wxml",
    "wxss"
  ],
  "git.ignoreLimitWarning": true,
  "minapp-vscode.cssExtname": "scss",
  "minapp-vscode.jsExtname": "ts",
  "minapp-vscode.wxmlFormatter": "prettier",
  "typescript.tsdk": "./node_modules/typescript/lib",
  "search.exclude": {
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/.temp/**": true
  },
  "stylelint.snippet": ["css", "scss"],
  "stylelint.validate": ["css", "scss"],
  "yaml.schemas": {
    "./res/.vscode/settings.schema.json": ["/res/config/**/settings.yml"],
    "./res/.vscode/wechat-account-data.schema.json": [
      "/res/data/account/*.yml"
    ],
    "./res/.vscode/qq-accounts.schema.json": [
      "/res/data/function/account/qq.yml"
    ],
    "./res/.vscode/wx-accounts.schema.json": [
      "/res/data/function/account/wx.yml"
    ],
    "./res/.vscode/map-markers.schema.json": [
      "/res/data/function/map/marker/*.yml"
    ],
    "./res/.vscode/tab-data.schema.json": ["/res/data/function/data/tab.yml"],
    "./res/.vscode/map-page-config.schema.json": [
      "/res/data/function/map/benbu/**/*.yml",
      "/res/data/function/map/jingyue/**/*.yml"
    ],
    "./res/.vscode/page-config.schema.json": ["/res/pages/**/*.yml"]
  }
}
