{
  "cSpell.words": [
    "benbu",
    "canonicalize",
    "danmu",
    "damnu",
    "darkmode",
    "errcode",
    "errmsg",
    "frameset",
    "innenu",
    "inited",
    "jingyue",
    "jfif",
    "miniapp",
    "miniprogram",
    "mptool",
    "nenu",
    "nenuyouth",
    "notifyed",
    "qqcode",
    "qqid",
    "qrcode",
    "qzone",
    "rmvb",
    "stylelint",
    "wechat",
    "weixin",
    "werun",
    "weui",
    "wxcode",
    "wxid",
    "wxss",
    "wxml"
  ],
  "git.ignoreLimitWarning": true,
  "minapp-vscode.cssExtname": "scss",
  "minapp-vscode.jsExtname": "ts",
  "minapp-vscode.wxmlFormatter": "prettier",
  "typescript.tsdk": "./node_modules/typescript/lib",
  "search.exclude": {
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/.temp/**": true
  },
  "yaml.schemas": {
    "./server/.vscode/settings.schema.json": ["/server/config/**/settings.yml"],
    "./server/.vscode/wechat.schema.json": ["/server/data/account/*.yml"],
    "./server/.vscode/qq-account.schema.json": [
      "/server/data/function/account/qq.yml"
    ],
    "./server/.vscode/wx-account.schema.json": [
      "/server/data/function/account/wx.yml"
    ],
    "./server/.vscode/tab-data.schema.json": [
      "/server/data/function/data/tab.yml"
    ],
    "./server/.vscode/location.schema.json": [
      "/server/data/function/map/benbu/**/*.yml",
      "/server/data/function/map/jingyue/**/*.yml"
    ],
    "./server/.vscode/marker.schema.json": [
      "/server/data/function/map/marker/*.yml"
    ],
    "./server/.vscode/page.schema.json": ["/server/pages/**/*.yml"]
  }
}
