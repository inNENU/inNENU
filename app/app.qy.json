      "pages": [
        "activate/activate",
        "archive/create",
        "archive/view",
        "calendar/calendar",
        "course/change-major",
        "course/exam-place",
        "grade/grade",
        "course/table",
        "email/apply",
        "email/email",
        "enroll/admission",
        "enroll/grade",
