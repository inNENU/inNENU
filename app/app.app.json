  "pages": [
    "pages/main/main",
    "pages/function/function",
    "pages/guide/guide",
    "pages/intro/intro",
    "pages/user/user",
    "pages/info/info",
    "pages/action/action",
    "pages/search/search",
    "pages/action/action",
    "pages/web/web",
    "settings/about/about",
    "settings/account/account",
    "settings/storage/storage",
    "function/calendar/calendar",
    "function/course/grade",
    "function/course/table",
    "function/enroll/admission",
    "function/enroll/grade",
    "function/enroll/plan",
    "function/library/library",
    "function/location/location",
    "function/map/map",
