{
  "entryPagePath": "pages/main/main",
  "pages": [
    "pages/main/main",
    "pages/function/function",
    "pages/guide/guide",
    "pages/intro/intro",
    "pages/user/user",
    "pages/info/info",
    "pages/search/search",
    "pages/web/web",
    "settings/about/about",
    "settings/account/account",
    "settings/privacy/privacy",
    "settings/storage/storage"
  ],
  "subPackages": [
    {
      "root": "function",
      "pages": [
        "cal/cal",
        "calendar/calendar",
        "course/grade",
        "course/table",
        "enroll/admission",
        "enroll/grade",
        "enroll/plan",
        "location/location",
        "map/map",
        "music/music",
        "video/video",
        "pe-calculator/pe-calculator",
        "phone/phone",
        "select/select",
        "school-media/school-media",
        "school-media/wechat",
        "weather/weather",
        "website/website"
      ]
    }
  ],
  "preloadRule": {
    "pages/function/function": {
      "network": "all",
      "packages": ["function"]
    }
  },
  "window": {
    "navigationBarBackgroundColor": "@navBgColor",
    "navigationBarTextStyle": "@navTextStyle",
    "navigationBarTitleText": "in 东师",
    "navigationStyle": "custom",
    "backgroundColor": "@bgColor",
    "backgroundTextStyle": "@bgTxtStyle",
    "backgroundColorTop": "@bgColorTop",
    "backgroundColorBottom": "@bgColorBottom",
    "pageOrientation": "portrait"
  },
  "tabBar": {
    "custom": false,
    "color": "@tabColor",
    "selectedColor": "#2ecc71",
    "borderStyle": "@tabBorderStyle",
    "backgroundColor": "@tabBgColor",
    "position": "bottom",
    "list": [
      {
        "pagePath": "pages/main/main",
        "text": "首页",
        "iconPath": "@homeIcon",
        "selectedIconPath": "icon/tab/home.png"
      },
      {
        "pagePath": "pages/intro/intro",
        "text": "东师介绍",
        "iconPath": "@introIcon",
        "selectedIconPath": "icon/tab/intro.png"
      },
      {
        "pagePath": "pages/guide/guide",
        "text": "东师指南",
        "iconPath": "@guideIcon",
        "selectedIconPath": "icon/tab/guide.png"
      },
      {
        "pagePath": "pages/function/function",
        "text": "功能大厅",
        "iconPath": "@functionIcon",
        "selectedIconPath": "icon/tab/function.png"
      },
      {
        "pagePath": "pages/user/user",
        "text": "我的东师",
        "iconPath": "@userIcon",
        "selectedIconPath": "icon/tab/user.png"
      }
    ]
  },
  "networkTimeout": {
    "request": 10000,
    "connectSocket": 8000,
    "uploadFile": 8000,
    "downloadFile": 25000
  },
  "debug": false,
  "frameset": true,
  "resizable": true,
  "permission": {
    "scope.userLocation": {
      "desc": "您的位置信息将用于校园地图导航"
    }
  },
  "requiredBackgroundModes": ["audio"],
  "usingComponents": {
    "account": "/base/account/account",
    "audio-player": "/base/audio/audio",
    "action": "/base/action/action",
    "card": "/base/card/card",
    "carousel": "/base/carousel/carousel",
    "doc": "/base/doc/doc",
    "functional-list": "/base/functional-list/functional-list",
    "footer": "/base/footer/footer",
    "grid": "/base/grid/grid",
    "img": "/base/img/img",
    "list": "/base/list/list",
    "loading": "/base/loading/loading",
    "location": "/base/location/location",
    "navbar": "/base/navbar/navbar",
    "plain-text": "/base/plain-text/plain-text",
    "phone": "/base/phone/phone",
    "share": "/base/share/share",
    "table": "/base/table/table",
    "title": "/base/title/title",
    "video-player": "/base/video/video"
  },
  "style": "v2",
  "darkmode": true,
  "themeLocation": "styles/theme.json",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "singlePage": {
    "navigationBarFit": "float"
  }
}
